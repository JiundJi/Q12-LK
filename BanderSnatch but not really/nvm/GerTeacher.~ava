package nvm;

public class GerTeacher extends Teacher{
    public GerTeacher(Room room, String name) {
        super(room, name);
    }

    @Override
    public void discuss() {
        util.println("yes?");
        if((int) (Math.random() * 101) < 15) {
            xp += 5;
            util.println("uwu");
        } else {
            xp--;
            util.println("qwq");
        }
    }
}